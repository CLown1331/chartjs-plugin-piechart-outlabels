{
	"root": "./docs",
	"author": "neckster",
	"gitbook": "3.2.2",
	"structure": {
		"readme": "docs/README.md",
		"summary": "docs/SUMMARY.md"
	},
	"plugins": [
		"-lunr",
		"-search",
		"search-plus",
		"anchorjs"
	],
	"pluginsConfig": {
		"anchorjs": {
			"icon": "#",
			"placement": "left",
			"visible": "always"
		},
		"theme-default": {
			"showLevel": false,
			"styles": {
			}
		}
	}
}
